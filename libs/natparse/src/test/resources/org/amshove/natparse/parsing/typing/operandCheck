DEFINE DATA LOCAL
1 #A10 (A10)
1 #N10 (N10)
1 #DAT (D)
1 #TIME (T)
1 #ADYN (A) DYNAMIC
1 #C-DIRECTION (A1) CONST<'F'>
1 #A1 (A1)
1 #ARR-A10 (A10/*)
1 #GRP-ARR (1:*)
  2 #GRP-ARR-1 (A1)
  2 #GRP-ARR-2 (N1)
END-DEFINE

RESIZE ARRAY #ARR-A10 TO (1:10)
RESIZE ARRAY #GRP-ARR TO (1:10)
RESIZE ARRAY #A10 TO (1:10) /* !{D:ERROR:NPP037} Operand can't be scalar

EXPAND ARRAY #ARR-A10 TO (1:10)
EXPAND ARRAY #GRP-ARR TO (1:10)
EXPAND ARRAY #A10 TO (1:10) /* !{D:ERROR:NPP037} Operand can't be scalar

REDUCE ARRAY #ARR-A10 TO (1:10)
REDUCE ARRAY #ARR-A10 TO (1:10)
REDUCE ARRAY #A10 TO (1:10) /* !{D:ERROR:NPP037} Operand can't be scalar

RESIZE DYNAMIC #ADYN TO 200
RESIZE DYNAMIC #N10 TO 200 /* !{D:ERROR:NPP037} format must be A, U or B

EXPAND DYNAMIC #ADYN TO 200
EXPAND DYNAMIC #N10 TO 200 /* !{D:ERROR:NPP037} format must be A, U or B

REDUCE DYNAMIC #ADYN TO 200
REDUCE DYNAMIC #N10 TO 200 /* !{D:ERROR:NPP037} format must be A, U or B

COMPRESS #ADYN INTO #ARR-A10(*) /* !{D:ERROR:NPP037} compress target can not be an array

/* Examine direction
EXAMINE DIRECTION 'F' #ADYN FOR 'A' REPLACE 'B' /* 'F' is constant
EXAMINE DIRECTION #C-DIRECTION #ADYN FOR 'A' REPLACE 'B' /* constant
EXAMINE DIRECTION #A1 #ADYN FOR 'A' REPLACE 'B' /* scalar
EXAMINE DIRECTION #N10 #ADYN FOR 'A' REPLACE 'B' /* !{D:ERROR:NPP037} Direction must be Alphanumeric

IF #N10 = #A10 /* !{D:ERROR:NPP037} not same family
IGNORE
END-IF

IF #A10 = #N10 /* !{D:ERROR:NPP037} not same family
IGNORE
END-IF

IF #N10 = #N10
IGNORE
END-IF

IF #A10 = #A10
IGNORE
END-IF

IF #N10 = #DAT /* D <-> Numeric is fine in relationals
IGNORE
END-IF

IF #N10 = #TIME /* T <-> Numeric is fine in relationals
IGNORE
END-IF

END
